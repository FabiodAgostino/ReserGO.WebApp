@using MudBlazor

<div class="d-flex timeContainer">
    <div class="d-flex justify-content-center align-items-center cardTime" >
        <span class="timeText">@Time</span>
    </div>
</div>

@code {
    [Parameter]
    public string Time { get; set; }

    protected override void OnInitialized()
    {
        if (Time.Length <= 1)
            Time = "00:00";
    }

}
